  "dependencies": {
    "@types/ioredis": "^4.17.3",
    "ioredis": "^4.17.3"
  },
  "peerDependencies": {
    "graphile-worker": ">=0.6.0 <1"
  },
  "devDependencies": {
    "@types/pg": "^8.6.5",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.4.0",
    "pg": "^8.7.3",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
